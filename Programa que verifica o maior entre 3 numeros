/*
  Name: Programa que verifica o maior entre 3 numeros
  Copyright: -
  Author: Carlos Eduardo Silva
  Date: 20/09/15 19:11
  Description: Verificar qual é o maior de três números digitados.
*/

# include <stdio.h>
# include <stdlib.h>

int main(void){
    int a, b, c;
    
    printf("Digite 3 numeros:\n");
    scanf("%d %d %d", &a, &b, &c);
    
    if((a >= b) && (a >= c))
          printf("\nA = %d. Este eh o maior.\n\n", a);
    else
        if((b >= a) && (b >= c))
              printf("\nB = %d. Este eh o maior.\n\n", b);
        else
              printf("\nC = %d. Este eh o maior.\n\n", c);
                  
    system("pause");
    return 0;
}
