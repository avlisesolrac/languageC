# include <stdio.h>
# include <stdlib.h>

int main (void){
    int x, y, z;
    
    printf("Digite os valores para verificar o maior: ");
    scanf("%i %i %i", &x, &y, &z);
    
    if(x >= y && x >= z){
        printf("O maior e: %i\n", x);
    }else{
        if(y >= x && y >= z){
            printf("O maior e: %i\n", y);
        }else{
            printf("O maior e: %i", z);
        }
    }
    
    system("pause");
    return 0;
}
