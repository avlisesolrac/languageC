	}else{
		++qtdMulher;
	}
	
	printf("Qual o salario do entrevistado: ");
	scanf("%f", &salario);
	
	somaSalarios += salario;
	
	printf("\n\nA quantidade de mulher(es) entrevistadas foi de: %i\n"
			"A quantidade de homem(ns) entrevistados foi de: %i\n"
			"A soma total dos salarios coletados foi de: R$%.2f\n\n",
			qtdMulher, qtdHomem, somaSalarios);
	
